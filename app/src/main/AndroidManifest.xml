<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ryanpatiency.theunitedstateofchickens">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/listview_chicksTribe"
            android:name=".ChickenActivity" />
        <activity
            android:label="@string/listview_docksTribe"
            android:name=".DockActivity" />
        <activity
            android:label="@string/listview_pigsTribe"
            android:name=".PigActivity" />
        <activity
            android:label="@string/listview_othersTribe"
            android:name=".OtherTribeActivity"></activity>
    </application>

</manifest>